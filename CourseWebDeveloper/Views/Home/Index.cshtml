<form action="@Url.Action("Index", "Home")" method="get">
    
    <fieldset>
        <legend>Informações Pessoais</legend>

        <label for="nome">Nome:</label><input type="text" id="nome" value="Internauta" tabindex="1" /><br />

        <label for="sexo">Sexo:</label><br />
        <input type="radio" name="sexo" id="sexo" value="M" />Masculino<br />
        <input type="radio" name="sexo" id="sexo" value="M" />Feminino<br />

        <label for="senha">
            Senha:
            <input type="password" id="senha" /><br />
        </label>
    </fieldset>

    <fieldset>
        <legend>Informações Profissionais</legend>

        <label for="conhecimentos">Conhecimentos:</label><br />
        <input type="checkbox" name="conhecimentos" id="conhecimentos" value="Java" />Java<br />
        <input type="checkbox" name="conhecimentos" id="conhecimentos" value="Html" />Html<br />
        <input type="checkbox" name="conhecimentos" id="conhecimentos" value="css" />css<br />
        <input type="checkbox" name="conhecimentos" id="conhecimentos" value="xslt" />xslt<br />

        Currículo:<br />
        <input type="file" id="curriculo" /><br />

        <input type="submit" value="Enviar" /><br />
        <input type="reset" value="Limpar" /><br />
        <input type="button" value="Validar Formulário" /><br />

        <label for="resumo">Resumo:</label><br />
        <textarea cols="40" rows="6" id="resumo">Escreva seu resumo aqui</textarea><br />

    </fieldset>

    <fieldset>
        <legend>Outras Informações</legend>

        País:<br />
        <select id="pais">
            <option>Brasil</option>
            <option>Espanha</option>
            <option>EUA</option>
            <option>Japão</option>
            <option>Suíça</option>
        </select><br />

        Editores ou Navegadores:<br />
        <select id="en">
            <optgroup label="Editores">
                <option>Notepad++</option>
                <option>DreamWeaver</option>
                <option>Eclipse</option>
                <option>HTML-Kit</option>
            </optgroup>
            <optgroup label="Navegadores">
                <option>Firefox</option>
                <option>IE</option>
                <option>Safari</option>
                <option>Opera</option>
            </optgroup>
        </select>
    </fieldset>
</form>

